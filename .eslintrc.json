{
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
  "ignorePatterns": ["src/**/*.svg", "src/**/*.css"],
  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],

    "no-unused-vars": ["warn", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }]
  }
}
