{
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
  "ignorePatterns": ["src/**/*.svg", "src/**/*.css"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],

    "no-unused-vars": ["error", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }]
  }
}
